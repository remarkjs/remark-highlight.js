{
  "name": "remark-highlight.js",
  "version": "1.2.0",
  "description": "Highlight code blocks in Markdown files with highlight.js.",
  "main": "dist/index.js",
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "copy-fixtures": "ncp src/__tests__/fixtures dist/__tests__/fixtures",
    "lint": "eslint src",
    "prepublish": "de dist && babel src --out-dir dist --ignore /__tests__/",
    "test": "npm run lint && babel src --out-dir dist && npm run copy-fixtures && ava dist/__tests__"
  },
  "keywords": [
    "highlight",
    "syntax",
    "highlight.js",
    "markdown",
    "remark"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.9.1",
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015-loose": "^6.1.4",
    "babel-preset-stage-0": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "del-cli": "^0.1.2",
    "eslint": "^1.10.3",
    "ncp": "^2.0.0",
    "remark": "^3.0.0",
    "remark-html": "^2.0.0"
  },
  "homepage": "https://github.com/ben-eb/remark-highlight.js",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/remark-highlight.js",
  "dependencies": {
    "highlight.js": "^9.0.0",
    "unist-util-visit": "^1.0.0"
  }
}
